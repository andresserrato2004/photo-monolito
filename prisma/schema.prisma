generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id
  name      String
  gender    String
  career    String
  image     String?   // Changed from Bytes? to String? to store the S3 URL
  createdAt DateTime @default(now())
}
